bool check_elements(int arr[], int n, int A, int B)
	{
		// Your code goes here
		set<int>ans;
		int i;

		for(i=0;i<n;i++)
		{
		    if(arr[i]>=A && arr[i]<=B)
		    ans.insert(arr[i]);
		}
		
	    if(ans.size()==B-A+1)
	    {
	        return 1;
	    }
	    else
	    {
	        return 0;
	    }
	}
