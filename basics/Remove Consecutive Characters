 string removeConsecutiveCharacter(string S)
    {
        // code here.
        int i=0;
        string str;
        
        while(i<S.length()-1)
        {
            if(S[i+1]!=S[i])
            {
                str+=S[i];
            }
            i++;
        }
        
        str+=S[i];
        
        return str;
    }
