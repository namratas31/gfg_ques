pair<long long, long long> getMinMax(long long a[], int n) {
    long max=a[0];
    long min=a[0];
    pair<long long, long long> data;
    for(int i=0;i<n;i++)
    {
        if(a[i]>max)
        max=a[i];
        if(a[i]<min)
        min=a[i];
    }
    data.first = min;
    data.second = max;
    return data;
}
