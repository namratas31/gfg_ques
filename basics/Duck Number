bool check_duck(string N){
    //code
    int i,flag=0;
    
    for(i=0;i<N.length();i++)
    {
        if(flag==0)
        {
            if(N[i]!='0')
            {
                flag=1;
            }
        }
        else
        {
            if(N[i]=='0')
            return true;
        }
    }
    
    return false;
    
}
