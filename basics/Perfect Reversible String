int isReversible(string str, int n) { 
         //complete the function here
         string ans;
         
         ans=str;
         reverse(str.begin(),str.end());
         int i;
         
         for(i=0;i<str.length()/2;i++)
         {
             if(ans[i]!=str[i])
             return 0;
         }
         
         return 1;
    } 
