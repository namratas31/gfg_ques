int binary_to_decimal(string str)
		{
		    // Code here.
		    int x=str.length();
		    int sum=0;
		    int i=0;
		    int count=1;
		    while(i!=x)
		    {
		        if(str[i]=='1')
		        {
		          sum=sum+(1*pow(2,x-count));
		        }
		     i++;
		     count++;
		    }
		    return sum;
		}
