int countSCC(int a,int b)
    {
        // Write Your Code here
        int count=0;
        a=a/2;
        if(a==b)
        {
            return a;
        }
        else if(a>b)
        {
            a=a-b;
            count=b;
            if(a==0)
            {
            return count;
            }
            else 
            {
                count+=a/2;
                return count;
            }
        }
        else 
        {
            return a;
        }
    }
