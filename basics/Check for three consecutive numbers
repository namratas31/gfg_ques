 vector<int>consecutiveSum(int n) { 
        // code here
        
        int x;
        vector<int>ans;
        
        if(n%3==0)
        {
            x=n/3;
            ans.push_back(x-1);
            ans.push_back(x);
            ans.push_back(x+1);
        }
        else
        {
            ans.push_back(-1);
        }
        
        return ans;
    }
