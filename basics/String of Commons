 	string  common_String(string s1, string s2){
   	    // Code here
   	    string s="";
   	    set<char>ans;
   	    int i;
   	    
   	    for(i=0;i<s2.length();i++)
   	    {
   	        if(s1.find(s2[i])!=string::npos)
   	        ans.insert(s2[i]);
   	    }
   	    
   	    for(auto i:ans)
   	    {
   	        s+=i;
   	    }
   	    
   	    if(s=="")
   	    return "nil";
   	    else
   	    return s;
   	}    
