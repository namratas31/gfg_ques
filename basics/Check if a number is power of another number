int isPowerOfAnother(long long X, long long Y){
        // code here 
        long long i=X;
        
        if(X==1 && Y!=1)
        return 0;
        
        if(Y==1 && X>0)
        return 1;
        
        while(i<Y)
        {
            i*=X;
        }
        
        if(i==Y)
        return 1;
        
        return 0;
    }
