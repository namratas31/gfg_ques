void transpose(vector<vector<int> >& matrix, int n)
    { 
        // code here 
        int temp;
        
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<i;j++)
            {
                temp=matrix[i][j];
                matrix[i][j]=matrix[j][i];
                matrix[j][i]=temp;
            }
        }
        
    }
