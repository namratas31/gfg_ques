vector<long long> factorialNumbers(long long N)
    {
        // Write Your Code here
        vector<long long> fact;
        long long count=1,i;
        
        for(i=1;i<=N;i++)
        {
            count*=i;
            
            if(count<=N)
            {
                fact.push_back(count);
            }
            else
            {
                break;
            }
        }
        
        return fact;
    }
