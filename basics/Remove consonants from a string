string removeConsonants(string s){
    //complete the function:
    
    string ans="";
    int i;
    
    for(i=0;i<s.length();i++)
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u')
        {
            ans+=s[i];
        }
        
        if(s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U')
        {
            ans+=s[i];
        }
    }
    
    if(ans=="")
    return "No Vowel";
    else
    return ans;
    }
