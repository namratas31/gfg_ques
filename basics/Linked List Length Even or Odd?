int isLengthEvenOrOdd(struct Node* head)
 {
      //Code here
      struct Node*temp=head;
      int count=0;
      while(temp!=0)
      {
          count++;
          temp=temp->next;
      }
      if(count%2==0)
      {
          return 0;
      }
      else
      {
          return 1;
      }
 }
