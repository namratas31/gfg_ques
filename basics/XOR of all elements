vector<int> getXor(vector<int> A, int N)
    {
        // Write your Code here
        vector<int> B;
        long int i,sum=0;
        
        for(i=0;i<N;i++)
        {
            sum^=A[i];
        }
        
        for(i=0;i<N;i++)
        {
            B.push_back(sum^A[i]);
        }
        
        return B;
    }
