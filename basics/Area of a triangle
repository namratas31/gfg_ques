double findArea(int A,int B,int C){
        //code here
        double ans,s,sum;
        int ab,bc,ca;
        ab=A+B;
        bc=B+C;
        ca=C+A;
        
        if(C>ab || A>bc || B>ca)
        return 0;
        else
        {
            s=(A+B+C)/double(2);
            sum=s*(s-A)*(s-B)*(s-C);
            sum=sqrt(sum);
            
            return sum;
        }
    }
