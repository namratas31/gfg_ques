long long int modify (long long int N)
{
    // your code here
    string s=to_string(N);
    string ans;
    long long int i;
    
    for(i=0;i<s.length()-1;i++)
    {
        if(s[i+1]==s[i])
        continue;
        else
        ans.push_back(s[i]);
    }
    
    ans.push_back(s[i]);
    i=stoll(ans);
    
    return i;
}
