int oddAndEven(string S) {
        // code here
        int x=S.length();
        int i,sum1=0,sum2=0;
        
        for(i=0;i<x;i++)
        {
            if(i%2==0)
            sum1+=S[i]-48;
            else
            sum2+=S[i]-48;
        }
        
        if(sum1==sum2)
        return 1;
        else
        return 0;
        
