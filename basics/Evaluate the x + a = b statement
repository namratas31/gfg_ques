int Evaluate(string str){
   	    // Code here
   	    int x,y,val1,val2;
   	    
   	    x=str.find("+");
   	    y=str.find("=");
   	    
   	    string s1=str.substr(x+1,y-x-1);
   	    val1=stoi(s1);
   	    
   	    string s2=str.substr(y+1);
   	    val2=stoi(s2);
   	    
   	    return val2-val1;
    }    
