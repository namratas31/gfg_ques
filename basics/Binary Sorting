void binSort(int A[], int N)
{
   //Your code here
   
   /**************
    * No need to print the array
    * ************/
    int i,count=0;
    
    for(i=0;i<N;i++)
    {
        if(A[i]==0)
        count++;
    }
    
    for(i=0;i<count;i++)
    {
        A[i]=0;
    }
    
    for(i=count;i<N;i++)
    {
        A[i]=1;
    }
    
}
