bool isSubSequence(string A, string B) 
    {
        // code here
        int i,j=0;
        
        for(i=0;i<B.length();i++)
        {
            if(A[j]==B[i])
            {
                j++;
                if(j==A.length())
                return 1;
            }
        }
        
        return 0;
    }
