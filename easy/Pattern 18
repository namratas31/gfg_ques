void printTriangle(int n) {
        // code here
        char c='A';
        c+=n-1;
        
        char d=c;
        
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=i;j++)
            {
                cout<<d<<" ";
                d--;
            }
            cout<<endl;
            d=c;
        }
    }
