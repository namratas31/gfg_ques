class Solution{
public:	
	// Returns count buildings that can see sunlight
	int countBuildings(int h[], int n) {
	    // code here
	    int max=h[0],count=1;
	    for(int i=0;i<n;i++)
	    {
	        if(h[i]>max)
	        {
	            max=h[i];
	            count++;
	        }
	    }
	    return count;
	}
