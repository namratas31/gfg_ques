void countOddEven(int arr[], int sizeof_array)
    {
        
        // your code here
        int count=0,odd=0;
        for(int i=0;i<sizeof_array;i++)
        {
            if(arr[i]%2==0)
            {
                count++;
            }
            else
            {
                odd++;
            }
        }
       cout<<odd<<" "<<count<<endl;
        
    }
