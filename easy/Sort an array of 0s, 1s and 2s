void sort012(int a[], int n)
{
    int min=0,temp;
    for(int k=0;k<n;k++)
    {
    if(a[k]==0)
    {
        temp=a[min];
        a[min]=a[k];
        a[k]=temp;
        min++;
    }
    }
    for(int i=min;i<n;i++)
    {
        if(a[i]==1)
        {
            temp=a[min];
            a[min]=a[i];
            a[i]=temp;
            min++;
        }
    }
    
    
}
