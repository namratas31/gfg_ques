int Count(string S){
    //complete the function here
    
    int count=0;
    
    for(int i=0;i<S.length();i++)
    {
        if(S[i]>='a' && S[i]<='z')
        {
            count++;
        }
        else if(S[i]>='A' && S[i]<='Z')
        {
            count++;
        }
    }
    
    return count;
}
