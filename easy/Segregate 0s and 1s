void segregate0and1(int arr[], int n) {
        // code here
        int i,count0=0,count1=0;
        for(i=0;i<n;i++)
        {
            if(arr[i]==0)
            {
                count0++;
            }
            
        }
        for(i=0;i<count0;i++)
        {
            arr[i]=0;
        }
        for(i=count0;i<n;i++)
        {
            arr[i]=1;
        }
    }
