int doUnion(int a[], int n, int b[], int m)  {
        //code here
        set<int> val;
        int i;
        
        for(i=0;i<n;i++)
        {
            val.insert(a[i]);
        }
        
        for(i=0;i<m;i++)
        {
            val.insert(b[i]);
        }
        
        return val.size();
    }
