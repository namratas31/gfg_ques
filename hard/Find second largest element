int print2largest(int arr[], int arr_size)
    {
    	//code here.
    	int i,max1=-1,max2=-1;
    	for(i=0;i<arr_size;i++)
    	{
    	    if(arr[i]>max1)
    	    {
    	        max2=max1;
    	        max1=arr[i];
    	    }
    	    if(arr[i]<max1 && arr[i]>max2)
    	    {
    	        max2=arr[i];
    	    }
    	}
    	return max2;
    }
