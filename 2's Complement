string complement(string bi){
        // code here
        int n=bi.length(),i;
        
        for(i=n-1;i>=0;i--)
        {
            if(bi[i]=='1')
            break;
        }
        
        for(int k=0;k<i;k++)
        {
            if(bi[k]=='0')
            bi[k]='1';
            else
            bi[k]='0';
        }
        
        return bi;
    }
