int isDivisible(int N) {
        // code here
        int count=0;
        int temp=N;
        
        if(N==0)
        return 0;
        
        while(temp>0)
        {
            count+=temp%10;
            temp=temp/10;
        }
        
        if(N%count==0)
        {
            return 1;
        }
        
            return 0;
    }
