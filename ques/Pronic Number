vector<int> pronicNumbers(int N)
    {
        // Write Your Code here
        vector<int> nums;
        
        int num=0,i=0;
        
        while(num<N)
        {
            num=i*(i+1);
            if(num<=N)
            nums.push_back(num);
            i++;
        }
        
        return nums;
    }
