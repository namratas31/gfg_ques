void leftRotate(int arr[], int k, int n) 
	{ 
	   // Your code goes here
	   k=k%n;
	   
	   int i,arr2[n],j=0;
	   
	   for(i=k;i<n;i++)
	   {
	       arr2[j]=arr[i];
	       j++;
	   }
		 
	   for(i=0;i<k;i++)
	   {
	       arr2[j]=arr[i];
	       j++;
	   }
	   
	   for(i=0;i<n;i++)
	   {
	       arr[i]=arr2[i];
	   }  
	   
	}
