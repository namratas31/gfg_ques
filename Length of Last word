int findLength(string s){
    //code
    int n=s.length();
    int i,count=0,flag=0;
    
   for(i=n-1;i>=0;i--)
   {
       if(s[i]==' ' && flag==0)
       continue;
       else
       {
           flag=1;
           if(s[i]==' ')
           break;
           
           count++;
       }
   }
   
   return count;
    
}
