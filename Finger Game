int fingerCount(int N)
    {
        // Write Your Code here
        if(N%8==1)
        {
            return 1;
        }
        else if(N%8==2 || N%8==0)
        {
            return 2;
        }
        else if(N%8==3 || N%8==7)
        {
            return 3;
        }
        else if(N%8==4 || N%8==6)
        {
            return 4;
        }
        else
        {
            return 5;
        }
    }
